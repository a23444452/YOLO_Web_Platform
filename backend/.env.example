# YOLO Web Platform Backend Configuration
# Copy this file to .env and modify as needed

# API Configuration
YOLO_API_HOST=0.0.0.0
YOLO_API_PORT=8000
YOLO_API_RELOAD=false
YOLO_API_DEBUG=false

# Training Configuration
YOLO_TRAINING_DIR=/tmp/yolo_training
YOLO_MAX_CONCURRENT_TRAININGS=2
YOLO_DEFAULT_DEVICE=cpu
YOLO_DEFAULT_EPOCHS=100
YOLO_DEFAULT_BATCH_SIZE=16
YOLO_DEFAULT_IMAGE_SIZE=640

# CORS Configuration
# Use comma-separated list for multiple origins
# YOLO_CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Logging Configuration
YOLO_LOG_LEVEL=INFO
YOLO_LOG_FORMAT=text

# Model Configuration
# YOLO_MODEL_CACHE_DIR=/path/to/cache

# Security
YOLO_MAX_UPLOAD_SIZE_MB=100

# Performance
YOLO_WORKER_THREADS=4
